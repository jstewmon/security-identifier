{
  "defaultSeverity": "error",
  "extends": ["tslint:latest", "tslint-config-prettier"],
  "jsRules": {},
  "rules": {
    "no-console": true,
    "no-submodule-imports": [true, "uuid"],
    "indent": [true, "spaces", 2],
    "max-classes-per-file": [true, 5],
    "trailing-comma": [true, { "multiline": "always", "singleline": "never" }]
  },
  "rulesDirectory": []
}
